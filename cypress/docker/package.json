{
  "name": "leda",
  "version": "0.0.1-alpha.2",
  "description": "UI components library for React applications",
  "main": "dist/index.js",
  "types": "./external-types",
  "scripts": {
    "start": "npm run build:demo",
    "test": "npm run jest",
    "build": "tsc",
    "watch": "babel src/**/*.ts --watch --out-dir dist/lib --extensions \\\".ts,.tsx\\\"",
    "build:demo": "npm run addStyles && webpack-dev-server",
    "build:analyze": "webpack --mode=production --config webpack.analyze.js",
    "addStyles": "rimraf ./public/assets/ && mkdirp ./public/assets && rimraf public/img/esphere && mkdirp public/img/esphere && ncp ./node_modules/styles-esphere/ public/img/esphere && node-sass ./styles/ -o ./public/assets/css/",
    "tsc": "tsc",
    "lint": "eslint --color --ext .ts,.tsx -- src",
    "prepublishOnly": "npm run build",
    "check": "npm run lint && npm run test && npm run tsc",
    "sass": "node-sass ./styles/ -o ./public/assets/css/",
    "sass:watch": "node-sass --watch ./styles/ -o ./public/assets/css/",
    "sass:build": "node-sass ./styles/ -o ./dist/styles/",
    "jest": "jest --color",
    "cy:open": "cypress open",
    "cy:test": "cypress run"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run check"
    }
  },
  "license": "MIT",
  "dependencies": {
    "attr-accept": "1.1.3",
    "classnames": "2.2.6",
    "create-react-class": "15.6.3",
    "element-closest": "3.0.1",
    "element-scroll-polyfill": "1.0.1",
    "history": "4.10.1",
    "lodash": "4.17.15",
    "react-css-collapse": "3.6.0",
    "react-dropzone": "10.1.8",
    "react-slider": "0.11.2",
    "react-syntax-highlighter": "11.0.2",
    "svgxuse": "1.2.6"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-transform-modules-commonjs": "7.6.0",
    "@babel/plugin-transform-runtime": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.6.0",
    "@babel/runtime": "7.6.0",
    "@cypress/webpack-preprocessor": "4.1.0",
    "@types/classnames": "2.2.9",
    "@types/enzyme": "3.10.3",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.138",
    "@types/react": "16.9.2",
    "@types/react-css-collapse": "3.6.0",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "4.3.5",
    "@types/react-slider": "0.8.1",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "circular-dependency-plugin": "5.2.0",
    "core-js": "^3.2.1",
    "cypress": "^3.6.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.4.0",
    "eslint": "6.4.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-airbnb-typescript": "4.0.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-cypress": "2.6.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "husky": "3.0.5",
    "intl": "1.2.5",
    "jest": "^24.8.0",
    "jest-date-mock": "1.0.7",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "node-sass": "4.12.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router-dom": "5.0.1",
    "rimraf": "3.0.0",
    "shortid": "^2.2.15",
    "typescript": "^3.7.2",
    "webpack": "4.40.2",
    "webpack-bundle-analyzer": "3.5.0",
    "webpack-cli": "3.3.8",
    "webpack-dev-server": "^3.8.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-router-dom": "^5.0.1"
  }
}
